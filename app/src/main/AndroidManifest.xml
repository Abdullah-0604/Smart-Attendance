                <action android:name="com.example.joker.smartattendance.View_records_panel" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Modify_panel">
            <intent-filter>
                <action android:name="com.example.joker.smartattendance.Modify_panel" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Result_panel">
            <intent-filter>
                <action android:name="com.example.joker.smartattendance.Result_panel" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Add_main_panel">
            <intent-filter>
                <action android:name="com.example.joker.smartattendance.Add_main_panel" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ListCompaniesActivity" />
        <activity android:name=".AddCompanyActivity" />
        <activity android:name=".UserAreaActivity" />
        <activity android:name=".AddActivity"></activity>
    </application>

</manifest>